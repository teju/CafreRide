<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.caferiders">

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="false"
        android:icon="@drawable/cafe_logo"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@drawable/cafe_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LaunchScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignInSignUp" />
        <activity android:name=".SignUp" />
        <activity android:name=".LogIn" />
        <activity android:name=".HomePage" />
        <activity android:name=".ScheduleBooking" />
        <activity android:name=".BikeList" />
        <activity android:name=".BikeDetail" />
        <activity android:name=".ConfirmBooking"></activity>
    </application>

</manifest>